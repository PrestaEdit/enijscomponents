{
    "compilerOptions": {
      "outDir": "./built",
      "allowJs": true,
      "target": "es5",
      "baseUrl": "./",
      "paths": {
        "@app/*": ["js/app/*"],
        "@js/*": ["js/*"],
        "@pages/*": ["js/pages/*"],
        "@components/*": ["js/components/*"],
        "@scss/*": ["scss/*"],
        "@node_modules/*": ["node_modules/*"],
        "@vue/*": ["js/vue/*"],
        "@PSTypes/*": ["js/types/*"]
      },
    },
    "include": ["./_dev/src/**/*"]
  }